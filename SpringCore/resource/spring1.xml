<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" 
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="list" class="com.ashu.spring.assignment2.Question">
	<property name="questionId" value="101"></property>
	<property name="question" value="who r u"></property>
	<property name="answers">
	<list>
	<value>Fine</value>
	<value>Good</value>
	<value>V.good</value>
	</list>
	</property>
	</bean>
	
	<bean id="set" class="com.ashu.spring.assignment2.Question">
	<property name="questionId" value="101"></property>
	<property name="question" value="who r u"></property>
	<property name="answers">
	<set>
	<value>Superb</value>
	<value>Good</value>
	<value>Very good</value>
	</set>
	</property>
	</bean>
</beans>